{
  "name": "shri-ram-physio-invoicing",
  "version": "1.1.2",
  "description": "Physiotherapy clinic invoicing and patient management system",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "npm run prisma:generate && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "clean:win": "node scripts/prebuild-win.js",
    "build:win": "node scripts/build-win.js",
    "publish": "npm run dist:win",
    "postinstall": "prisma generate && electron-builder install-app-deps",
    "preview": "vite preview",
    "start": "electron .",
    "package": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "keywords": [
    "electron",
    "invoice",
    "physiotherapy",
    "react",
    "tailwindcss"
  ],
  "author": "Shree Ram Physiotherapy & Rehabilitation Centre",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Arinjain111/Shree-ram-physio.git"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0-alpha.25",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "electron": "^39.2.7",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.4.32",
    "prisma": "^7.1.0",
    "tailwindcss": "^4.0.0-alpha.25",
    "typescript": "^5.3.3",
    "vite": "^7.2.6",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-better-sqlite3": "^7.1.0",
    "@prisma/adapter-libsql": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.3.9",
    "googleapis": "^128.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "zod": "^4.1.13"
  }
}
